{
  "parser": "babel-eslint",
  "extends": ["airbnb", "eslint-plugin-react", "plugin:react-hooks/recommended"],
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off"
  }
}
